//package com.DataStructure.Sorted;
class Node<T>
{
	private int value;
	private Node<T> next;
	//returns node value
	public int getValue()
	{
		return value;
	}
	//returns next node reference
	public Node<T> getNext()
	{
		return next;
	}
	//set node value
	public void setValue(int value)
	{
		this.value=value;
	}
	//set node reference
	public void setNext(Node<T> next)
	{
		this.next=next;
	}

}
